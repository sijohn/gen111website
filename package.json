{
  "name": "shopnx",
  "version": "2.2.0",
  "description": "ShopNx using Angular4",
  "keywords": [
    "shopnx",
    "angular",
    "angular2",
    "angular4",
    "webpack",
    "typescript"
  ],
  "author": "Swadesh Behera <swadesh@codenx.com>",
  "homepage": "http://shopnx.in",
  "scripts": {
    "dev": "concurrently \"tsc -w -p server\" \"node-dev --no-notify dist/server/app.js\" \" npm run server:dev\" ",
    "server-only": "concurrently \"tsc -w -p server\" \"node-dev --no-notify dist/server/app.js\" ",
    "prod": "concurrently \"tsc -w -p server\" \" npm run build:prod && node copy-dist-files\" ",
    "start": "node server/app",
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && cross-env BUILD_AOT=1 npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot": "npm run build:aot:prod",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "build:prod": "npm run clean:dist && npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch --content-base client/",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
